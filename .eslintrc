{
  "rules": {
    "no-useless-escape": [0],
    "prefer-const": [2],
    "no-var": [2],
    "prefer-spread": [2],
    "array-bracket-spacing": [2],
    "no-confusing-arrow": [1],
    "brace-style": [2]
  },
  "env": {
    "browser": false,
    "es6": true
  },
  "extends": "standard",
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
